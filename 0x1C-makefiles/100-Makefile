CCWITHFLAGS=gcc -Wall -Werror -Wextra -pedantic
SRC=school.c main.c
OBJ= $(SRC:.c=.o)
NAME=school
CFLAGS=-Wall -Werror -Wextra -pedantic
RM=rm -f
.PHONY=all clean ocean fclean re
all: school.h $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

clean:
	$(RM) .*~ *~ *.swp $(NAME)

oclean:
	$(RM) $(OBJ)

fclean: clean oclean

re: fclean all